    "qrcode": "^1.5.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node16": "^16.1.1",
    "@types/node": "16.x",
    "@types/qrcode": "^1.5.2",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-check-updates": "^16.13.2",
    "npm-run-all": "^4.1.5",
